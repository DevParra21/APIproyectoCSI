package com.proyectocsi.api.rest.models.services;

import java.util.List;

import org.springframework.stereotype.Service;

import com.proyectocsi.api.rest.models.dao.IInscripcionDao;
import com.proyectocsi.api.rest.models.entity.Grupo;

@Service
public class InscripcionImpl implements IInscripcionService {

	private IInscripcionDao inscripcionDao;
	@Override
	public void InsertInscripcion(Long matricula, Long grupo) {
		// TODO Auto-generated method stub
		this.inscripcionDao.InsertInscripcion(matricula, grupo);
	}

	@Override
	public void DeleteInscripcion(Long matricula, Long grupo) {
		// TODO Auto-generated method stub
		this.inscripcionDao.DeleteInscripcion(matricula, grupo);
	}

	@Override
	public List<Grupo> findAllByNotRegistered(Long idMatricula) {
		// TODO Auto-generated method stub
		return this.inscripcionDao.findAllByNotRegistered(idMatricula);
	}

	@Override
	public List<Grupo> findAllByRegistered(Long idMatricula) {
		// TODO Auto-generated method stub
		return this.inscripcionDao.findAllByRegistered(idMatricula);
	}

}
