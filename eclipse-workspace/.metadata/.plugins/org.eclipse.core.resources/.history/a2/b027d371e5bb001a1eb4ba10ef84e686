package com.proyectocsi.api.rest.controllers;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.proyectocsi.api.rest.models.entity.TipoProfesor;
import com.proyectocsi.api.rest.models.services.ITipoProfesor;

@RestController
@RequestMapping("/api")
public class TipoProfesorController {
	
	@Autowired
	private ITipoProfesor tipoProfesor;
	
	@GetMapping("/tipoprofesor")
	public List<TipoProfesor> index(){
		return tipoProfesor.findAll();
	}
}
